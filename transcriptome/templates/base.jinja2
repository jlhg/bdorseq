<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN">

<head>
  {% block head %}
    {% load staticfiles %}
    <link rel="stylesheet" href="{{ STATIC_URL ~ 'css/style.css' }}" />
    <link href='http://fonts.googleapis.com/css?family=Droid+Sans+Mono' rel='stylesheet' type='text/css'>
    {# <script type="text/javascript" src="{{ STATIC_URL ~ 'js/main.js' }}"></script> #}

    <title>
      {% block title %}
        Home
      {% endblock title %}
      - Oriental Fruit Fly Transcriptome Database
    </title>

  {% endblock head %}
</head>

<body>
  {% block body %}

  <div id="header">
    {% block header %}
      <p>Oriental Fruit Fly Database</p>
    {% endblock header %}
  </div>

  <div id="menu">
    {% block menu %}
      <p><a href="/bdorseq/">Home<a> |
            <a href="/bdorseq/archive/">Archive</a> |
            <a href="/bdorseq/search/">Download</a> |
            <a href="/bdorseq/logout/">Logout</a></p>
    {% endblock menu %}
  </div>

  <div id="content">
    {% block content %}
    {% endblock content %}
  </div>

  <div id="footer">
    {% block footer %}
    <p>Oriental Fruit Fly Transcriptome Database v1.4</p>
    {% endblock footer %}
  </div>

  {% endblock body %}
</body>
