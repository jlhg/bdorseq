{% extends "transcriptome/base.jinja2" %}

{% block menu %}
  <p><a href="/bdorseq/logout/">Logout</a></p>
{% endblock menu %}

{% block content %}
  {% block archive %}
    <h2>Archive</h2>
    {% include "transcriptome/archive_search_form.jinja2" %}
  {% endblock archive %}

  {% block search %}
    <h2>Browse: Transcript</h2>
    {% include "transcriptome/transcript_search_form.jinja2" %}
  {% endblock search %}

  {% block seqvar %}
    <h2>Browse: Sequence Variation</h2>
    {% include "transcriptome/sequence_variation_search_form.jinja2" %}
  {% endblock seqvar %}

  {% block releases %}
    <h2>Releases (FTP)</h2>
    Go <a href="{{ FTP_URL }}" target="_blank">here</a> for large file download.
  {% endblock releases %}

{% endblock content %}
